extends ../layout

block content
    h3 所有作业
    table(class="table table-hover" style="margin-top:30px;")
        each item in list
            tr
                td
                    a(href="./assignment/#{item['id']}",class="link",style="color:#222;font-size:16px")
                        span(class="glyphicon glyphicon-folder-close")
                        span(style="margin-left:5px;") #{item["name"]}
